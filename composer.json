{
    "name": "simplecms/region",
    "description": "SimpleCMS/Laravel Region Component / 中国行政地理划分组件",
    "keywords": [
        "laravel",
        "region",
        "中国行政地理划分组件",
        "simplecms"
    ],
    "version": "1.0.7",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SimpleCMS\\Region\\": "src/"
        }
    },
    "authors": [
        {
            "name": "hackout",
            "email": "hackout@vip.qq.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=9.0",
        "ext-mysqli": ">=5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8",
        "simplecms/framework": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleCMS\\Region\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleCMS\\Region\\RegionServiceProvider"
            ],
            "aliases": {
                "Region": "SimpleCMS\\Region\\Facades\\Region"
            }
        }
    }
}